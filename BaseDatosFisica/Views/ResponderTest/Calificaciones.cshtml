@model IEnumerable<BaseDatosFisica.Models.TestEstudiante>
@{
    ViewBag.Title = "Calificaciones";
}
@if(Model.Count() == 0)
{
    <h2>Este estudiante no ha hecho ningún Test</h2>
}
else
{
    <h1>@Model.First().estudiante.Nombre @Model.First().estudiante.Apellidos</h1>
    <br />
    <h2>Test Realizados:</h2>
    foreach (var item in Model)
    {
        <p>@Html.ActionLink(item.test.Nombre, "Nota", new { idTestEstudiante = item.TestEstudianteID })</p>
        
    }
}

